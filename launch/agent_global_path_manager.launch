<launch>
 <node pkg="nav_scripts" type="follow_global_path.py" name="$(anon follow_global_path)" output="log">
	<param name="num_obsts" value= "$(arg num_obsts)" />
	<param name="world" value = "$(arg world)" />
	<param name="controller" value = "$(arg controller)" />
 	<param name="seed" value = "$(arg seed)" />
	<param name="start_xs" value = "$(arg start_xs)" />
	<param name="start_ys" value = "$(arg start_ys)" />
	<param name="goal_xs" value = "$(arg goal_xs)" />
	<param name="goal_ys" value = "$(arg goal_ys)" />

 </node>
</launch>
